<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="org.footware.server.db">
	<class name="Track" table="track">
		<id column="id" name="id">
			<generator class="native"></generator>
		</id>
	</class>
	<set name="track" table="track" lazy="true">
		<key column="id">
		<one-to-many column="track_id" class="Track">
	</set>
	<set name="user" table="user" lazy="true">
		<key column="id">
		<one-to-many column="user_id" class="User">
	</set>
	<property name="text"/>
	
</hibernate-mapping>
